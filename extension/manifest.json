{
	"name" : "Hacker News Toolbar",
	"version" : "1.0",
	"manifest_version" : 2,
	"description" : "A toolbar for Hacker News, like the Reddit Toolbar",
	"content_scripts" : [
		{
			"matches" : [
				"http://news.ycombinator.com/",
				"http://news.ycombinator.com/x*",
				"http://news.ycombinator.com/news"
			],
			"css" : [],
			"js" : ["jquery.js", "hntoolbar.js"]
		}
	]
}